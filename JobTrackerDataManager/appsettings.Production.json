{
  "ConnectionStrings": {
    "Default": "Added in CI/CD by Azure DevOps"
  },
  "Jwt": {
    "Key": "Added in CI/CD by Azure DevOps",
    "Issuer": "https://localhost:7065",
    "Audience": "https://localhost:7065",
    "TokenValidityInMinutes": "15",
    "RefreshTokenValidityInDays": 7
  },
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Datadog.Logs",
      "Serilog.Exceptions"
    ],
    "MinimumLevel": "Debug",
    "WriteTo": [
      {
        "Name": "DatadogLogs",
        "Args": {
          "apiKey": "Added in CI/CD by Azure DevOps",
          "source": "JobTrackerProduction",
          "configuration": {
            "url": "https://http-intake.logs.datadoghq.eu"
          }
        }
      }
    ],
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithExceptionDetails",
      "WithThreadId"
    ],
    "Properties": {
      "Application": "JobTracker"
    }
  }
}